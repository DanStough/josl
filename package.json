{
  "name": "josl",
  "version": "0.0.1",
  "description": "The Jenkins OS Light. Another Jenkins build status monitor, now with more flavor!",
  "main": "lib/application.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "debug": "DEBUG=* npm start",
    "debug-demo": "DEBUG=* npm run start-demo",
    "lint": "standard --fix --verbose | snazzy",
