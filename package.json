  "bugs": {
    "url": "https://github.com/DanStough/josl/issues"
  },
  "homepage": "https://github.com/DanStough/josl#readme",
  "devDependencies": {
    "ava": "^0.20.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "config": "^1.26.1",
    "death": "^1.1.0",
    "debug": "^2.6.8",
    "lodash": "^4.17.4",
    "raspi-gpio": "^4.0.2",
    "raspi-i2c": "^5.0.4",
    "rxjs": "^5.4.2"
